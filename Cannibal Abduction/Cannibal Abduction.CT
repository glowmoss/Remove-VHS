    <CheatEntry>
      <ID>95</ID>
      <Description>"Attach Process"</Description>
      <Color>00FFFF</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>{$lua}
if syntaxcheck then return end
[ENABLE]
Proc="Cannibal Abduction.exe"
Nam="Cannibal Abduction"
if  openProcess(Proc) then
        MessageDialog(Nam, "Attached to " .. Nam, 2,mbOK) end
  if not getProcessIDFromProcessName(Proc) then
       error(MessageDialog(Nam, Nam .." Isn't Running\rPlease Open " .. Nam .. " And Try Again",1,mbOK )) end
return 'nop'
[DISABLE]
</AssemblerScript>
