[ENABLE]
function cycleFullCompact(sender,force); local state = not(compactmenuitem.Caption == 'Compact'); if force~=nil then state = not force end; compactmenuitem.Caption = state and 'Compact' or 'Full'; getMainForm().Splitter1.Visible = state; getMainForm().Panel4.Visible = state; getMainForm().Panel5.Visible = state; end; function addCompactMenu(); if compactmenualreadyexists then return end; local parent = getMainForm().Menu.Items; compactmenuitem = createMenuItem(parent); parent.add(compactmenuitem); compactmenuitem.Caption = 'Compact'; compactmenuitem.OnClick = cycleFullCompact; compactmenualreadyexists = 'yes'; end; addCompactMenu(); cycleFullCompact(nil,true)
[DISABLE]
cycleFullCompact(nil,false)
</AssemblerScript>
